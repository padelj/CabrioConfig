<?xml version="1.0" ?>
<cabrio-config>	
  <interface>
    <full-screen>true</full-screen>
    <theme>carousel</theme>
    <screen>
      <width>1366</width>
      <height>768</height>
      <rotation>0</rotation>
      <flip-horizontal>0</flip-horizontal>
      <flip-vertical>0</flip-vertical>
    </screen>
    <graphics>
      <quality>high</quality>
      <max-image-width>512</max-image-width>
      <max-image-height>512</max-image-height>
    </graphics>
    <controls>
      <event>
        <name>up</name>
        <value>up</value>
        <device>
          <type>keyboard</type>
          <id>0</id>
        </device>
      </event>
      <event>
        <name>down</name>
        <value>down</value>
        <device>
          <type>keyboard</type>
          <id>0</id>
        </device>
      </event>
      <event>
        <name>left</name>
        <value>left</value>
        <device>
          <type>keyboard</type>
          <id>0</id>
        </device>
      </event>
      <event>
        <name>right</name>
        <value>right</value>
        <device>
          <type>keyboard</type>
          <id>0</id>
        </device>
      </event>
      <event>
        <name>select</name>
        <value>lctrl</value>
        <device>
          <type>keyboard</type>
          <id>0</id>
        </device>
      </event>
      <event>
        <name>back</name>
        <value>space</value>
        <device>
          <type>keyboard</type>
          <id>0</id>
        </device>
      </event>
      <event>
        <name>quit</name>
        <value>escape</value>
        <device>
          <type>keyboard</type>
          <id>0</id>
        </device>
      </event>
    </controls>
  </interface>
  <locations>
    <location>
      <type>screenshot</type>
      <directory>/home/jim/.mame/snap</directory>
    </location>
  </locations>
  <emulators>
    <emulator>
      <name>mame</name>
      <display-name>MAME</display-name>
      <executable>mame</executable>
      <params>
        <param>
          <name>-nowindow</name>
        </param>
        <param>
          <name>-skip_gameinfo</name>
        </param>
        <param>
          <name>-joystick</name>
        </param>
      </params>
    </emulator>
  </emulators>
  <game-list>
    <name>M.A.M.E.</name>
    <games>
      <game>
        <name>Pac Man</name>
        <platform>Arcade</platform>
        <rom-image>pacman.zip</rom-image>
        <images>
          <image>
            <type>screenshot</type>
            <image-file>pacman/0000.png</image-file>
          </image>
        </images>
        <categories>
          <category>
            <name>Genre</name>
            <value>Maze</value>
          </category>
        </categories>
      </game>
      <game>
        <name>Ms. Pac Man</name>
        <platform>Arcade</platform>
        <rom-image>mspacman.zip</rom-image>
        <images>
          <image>
            <type>screenshot</type>
            <image-file>mspacman/0000.png</image-file>
          </image>
        </images>
        <categories>
          <category>
            <name>Genre</name>
            <value>Maze</value>
          </category>
        </categories>
      </game>
      <game>
        <name>Dragon Breed</name>
        <platform>Arcade</platform>
        <rom-image>dbreed.zip</rom-image>
        <images>
          <image>
            <type>screenshot</type>
            <image-file>dbreed/0000.png</image-file>
          </image>
        </images>
        <categories>
          <category>
            <name>Genre</name>
            <value>Scroller Shooter</value>
          </category>
        </categories>
      </game>
    </games>
  </game-list>
</cabrio-config>